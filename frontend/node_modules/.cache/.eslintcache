onfirmPassword) {\r\n            // prevents the error from stacking multiple times\r\n            if (!toast.isActive(toastId.current)) {\r\n                toastId.current = toast.error(\"Password does not match\");\r\n              }\r\n        } else {\r\n            const { name, email, password } = formData\r\n            dispatch(updateUserProfile({id: user._id, name, email, password}))\r\n            if (!toast.isActive(toastId.current)) {\r\n                toastId.current = toast.success(\"Profile Updated\");\r\n              }\r\n        }\r\n    }\r\n\r\n    return (\r\n        <>\r\n            <Toast />\r\n            {error && <Message variant=\"alert-danger\">{error}</Message>}\r\n            {updateLoading && <Loading />}\r\n            <form className=\"profile-update-form\" onSubmit={handleSubmit}>\r\n                    <label>\r\n                            Name:\r\n                            <input \r\n                                type=\"text\" \r\n                                name=\"name\"\r\n                                // value defaults to empty string to prevent uncontrolled to controlled input error \r\n                                value={formData.name || \"\"} \r\n                                onChange={(e) => handleChange(e)}\r\n                                required \r\n                            />\r\n                        </label>\r\n\r\n                        <label>\r\n                            Email:\r\n                            <input \r\n                                type=\"email\" \r\n                                name=\"email\" \r\n                                value={formData.email || \"\"} \r\n                                onChange={(e) => handleChange(e)}\r\n                                required \r\n                            />\r\n                        </label>\r\n\r\n                        <label>\r\n                            Password:\r\n                            <input \r\n                                type=\"password\" \r\n                                name=\"password\" \r\n                                value={formData.password || \"\"} \r\n                                onChange={(e) => handleChange(e)}\r\n                                required \r\n                            />\r\n                        </label>\r\n\r\n                        <label>\r\n                            Confirm Password:\r\n                            <input \r\n                                type=\"password\" \r\n                                name=\"confirmPassword\" \r\n                                value={formData.confirmPassword || \"\"} \r\n                                onChange={(e) => handleChange(e)}\r\n                                required \r\n                            />\r\n                        </label>\r\n                        <button>UPDATE PROFILE</button>\r\n                    </form>\r\n        </>\r\n    )\r\n}\r\n\r\nexport default ProfileUpdateForm","C:\\Users\\Cody_\\Documents\\GitHub\\project\\ramseys-craft-store\\frontend\\src\\Components\\LoadingError\\Toast.js",[],[],"C:\\Users\\Cody_\\Documents\\GitHub\\project\\ramseys-craft-store\\frontend\\src\\Screens\\ShippingScreen.jsx",[],[],"C:\\Users\\Cody_\\Documents\\GitHub\\project\\ramseys-craft-store\\frontend\\src\\Screens\\PaymentScreen.jsx",[],[],"C:\\Users\\Cody_\\Documents\\GitHub\\project\\ramseys-craft-store\\frontend\\src\\Screens\\PlaceOrderScreen.jsx",[],[],"C:\\Users\\Cody_\\Documents\\GitHub\\project\\ramseys-craft-store\\frontend\\src\\utils.js",[],[],"C:\\Users\\Cody_\\Documents\\GitHub\\project\\ramseys-craft-store\\frontend\\src\\Components\\ProductCard.jsx",[],[],"C:\\Users\\Cody_\\Documents\\GitHub\\project\\ramseys-craft-store\\frontend\\src\\Screens\\guestCheckoutScreen.jsx",[],[],"C:\\Users\\Cody_\\Documents\\GitHub\\project\\ramseys-craft-store\\frontend\\src\\Screens\\GuestCheckoutScreen.jsx",["233"],[],"import { useState, useEffect } fr